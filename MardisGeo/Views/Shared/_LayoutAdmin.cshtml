            <script>
                function ModalCambio(ccos) {
                $("#idccn").val("");
                $('#idcc').val("");
                $("#lidcc").hide();
                $("#lidccs").hide();
                    $('#modal-default').modal('show');

            }

            $("#btnm").click(function () {
                $("#lidcc").hide();
                $("#lidccs").hide();

                if ($('#idcc').val() == $('#idccn').val()) {


                    var sec = $("#idccn").val();
                    $.ajax({
                        url: '@Url.Action("UpdatePass", "Login")',
                        type: "GET",
                        content: "application/json; charset=utf-8",
                        data: {
                            newpass: sec

                        },
                        success: function (data) {
                            if (data == 1) {
                                $("#lidccs").show();
                                $("#idccn").val("") ;
                                $('#idcc').val("")  ;
                            }
                        },

                        error: function () {


                        }


                    });

                } else {
                    $("#lidcc").show();

                }



            });
            </script>
